{
  "name": "nodejs-starter",
  "version": "0.0.1",
  "description": "Quick NodeJs Starter.",
  "author": "Viktoriia Hrechukha<viktoriia.hrechukha@gmail.com>",
  "private": true,
  "engines": {
    "node": "16.10",
    "npm": "8.3.1"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node ./dist/main.js",
    "start:dev": "ENV=local nest start --watch",
    "start:debug": "ENV=local nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose kill -d",
    "typeorm": "node --require ts-node/register --require tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/infrastructure/ormconfig.ts",
    "migrations:generate": "npm run typeorm migration:generate -- -p -n ${1}",
    "migrations:up": "npm run typeorm migration:run",
    "migrations:down": "npm run typeorm migration:revert",
    "prepare": "husky install",
    "postinstall": "npm run prepare",
    "postci": "npm run prepare",
    "unused": "ts-prune"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.4",
    "@nestjs/event-emitter": "^1.1.1",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/platform-socket.io": "^8.4.4",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "<=8.0.3",
    "@nestjs/websockets": "^8.4.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "husky": "^7.0.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.34",
    "pg": "^8.7.3",
    "pg-connection-string": "^2.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "socket.io": "^4.5.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "<=0.2.45"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.6.2",
    "socket.io-client": "^4.5.0",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.7.0",
    "ts-prune": "^0.10.3",
    "typescript": "^4.7.2"
  }
}
